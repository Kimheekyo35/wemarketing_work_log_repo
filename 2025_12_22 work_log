import nodriver as nd

async def main():
    browser = await nd.launch(
        headless=False,           # ê°€ëŠ¥í•˜ë©´ headed
        user_data_dir="/tmp/nd-profile",
        args=[
            "--no-sandbox",
            "--disable-dev-shm-usage",
        ]
    )

    page = await browser.get("https://target-site.com")
    await page.sleep(10)

nd.run(main)
